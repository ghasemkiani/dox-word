<cmn:g xmlns="http://www.w3.org/1999/xhtml" xmlns:cmn="urn:dox:cmn" xmlns:b="urn:dox:base" xmlns:wrd="urn:dox:word"><cmn:script>context.type = renderer.x.attr(context.templateComponent.node, "type") || "line";</cmn:script><cmn:g xmlns:v="urn:schemas-microsoft-com:vml" xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:w="urn:schemas-microsoft-com:office:word" xmlns:m="http://schemas.microsoft.com/office/2004/12/omml"><b:if cond="{context.type === 'line'}"><br clear="all"/></b:if><b:if cond="{context.type === 'page'}"><br clear="all" style="mso-special-character:line-break;page-break-before:always;"/></b:if><b:if cond="{context.type === 'column'}"><br clear="all" style="mso-special-character:line-break;mso-column-break-before:always;"/></b:if><b:if cond="{context.type === 'section' || context.type === 'section.page'}"><br clear="all" style="page-break-before:always;mso-break-type:section-break;"/></b:if><b:if cond="{context.type === 'section.continuous'}"><br clear="all" style="page-break-before:auto;mso-break-type:section-break;"/></b:if><b:if cond="{context.type === 'section.even'}"><br clear="all" style="page-break-before:left;mso-break-type:section-break;"/></b:if><b:if cond="{context.type === 'section.odd'}"><br clear="all" style="page-break-before:right;mso-break-type:section-break;"/></b:if></cmn:g></cmn:g>
