				<w:SplitPgBreakAndParaMark/>
			</w:Compatibility>
			<m:mathPr>
				<m:mathFont><cmn:script>renderer.x.attr(node, "m:val", context.docw.fontMath);</cmn:script></m:mathFont>
				<m:brkBin m:val="before"/>
				<m:brkBinSub m:val="--"/>
				<m:smallFrac/>
				<m:dispDef/>
				<m:lMargin m:val="0"/>
				<m:rMargin m:val="0"/>
				<m:defJc m:val="centerGroup"/>
				<m:wrapIndent m:val="1440"/>
				<m:intLim m:val="subSup"/>
				<m:naryLim m:val="undOvr"/>
			</m:mathPr>
		</w:WordDocument>
	</xml>
	<wrd:styles/>
	<xml>
		<o:shapedefaults v:ext="edit" spidmax="1027"/>
	</xml>
	<xml>
		<o:shapelayout v:ext="edit">
			<o:idmap v:ext="edit" data="1"/>
		</o:shapelayout>
	</xml>
</head>

<body lang="EN-US" link="black" vlink="purple" style="word-wrap:break-word">
	<cmn:script>
		renderer.x.css(node, {"tab-interval": context.docw.tabInterval});
		if (context.docw.bgcolor) {
			renderer.x.attr(node, "bgcolor", context.docw.bgcolor);
		}
	</cmn:script>
	<cmn:script>context.docw.d.nbody = node;</cmn:script>
</body>

