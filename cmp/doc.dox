<cmn:g xmlns="http://www.w3.org/1999/xhtml" xmlns:cmn="urn:dox:cmn" xmlns:b="urn:dox:base" xmlns:wrd="urn:dox:word">
<cmn:script>
	let lang = renderer.x.attr(context.templateComponent.node, "lang") || "fa-IR";
	let rtl = /^(fa|ar|he)/i.test(lang);
	let dir = rtl ? "rtl" : "ltr";
	let bgcolor = renderer.x.attr(context.templateComponent.node, "bgcolor") || "";
	let title = renderer.x.attr(context.templateComponent.node, "title") || "";
	let author = renderer.x.attr(context.templateComponent.node, "author") || "Ghasem Kiani";
	let fontMain = renderer.x.attr(context.templateComponent.node, "font-main") || "GBadr";
	let fontHeadings = renderer.x.attr(context.templateComponent.node, "font-headings") || "GJalal";
	let fontCode = renderer.x.attr(context.templateComponent.node, "font-code") || "Consolas";
	let fontMath = renderer.x.attr(context.templateComponent.node, "font-math") || "GNazaninMath";
	let tabInterval = renderer.x.attr(context.templateComponent.node, "tab-interval") || "5mm";
	let width = renderer.x.attr(context.templateComponent.node, "width") || "148mm";
	let height = renderer.x.attr(context.templateComponent.node, "height") || "210mm";
	let margin = renderer.x.attr(context.templateComponent.node, "margin");
	let marginY = renderer.x.attr(context.templateComponent.node, "margin-y") || margin || "25mm";
	let marginTop = renderer.x.attr(context.templateComponent.node, "margin-top") || marginY;
	let marginBottom = renderer.x.attr(context.templateComponent.node, "margin-bottom") || marginY;
	let marginX = renderer.x.attr(context.templateComponent.node, "margin-x") || margin || "24mm";
	let marginStart = renderer.x.attr(context.templateComponent.node, "margin-start") || marginX;
	let marginEnd = renderer.x.attr(context.templateComponent.node, "margin-end") || marginX;
	let marginHeader = renderer.x.attr(context.templateComponent.node, "margin-header") || "10mm";
	let marginFooter = renderer.x.attr(context.templateComponent.node, "margin-footer") || "10mm";
	let orientation = renderer.x.attr(context.templateComponent.node, "orientation") || "portrait";
	context.templateComponent.context.docw = {
		lang,
		rtl,
		dir,
		bgcolor,
		title,
		author,
		fontMain,
		fontHeadings,
		fontCode,
		fontMath,
		tabInterval,
		width,
		height,
		margin,
		marginX,
		marginY,
		marginTop,
		marginBottom,
		marginStart,
		marginEnd,
		marginHeader,
		marginFooter,
		orientation,
		d: {
			nhtml: null,
			nhead: null,
			nbody: null,
		},
		h: {
			nhtml: null,
			nhead: null,
			nbody: null,
		},
		sections: [],
	};
</cmn:script>
<!-- document -->
<html xmlns:v="urn:schemas-microsoft-com:vml" xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:w="urn:schemas-microsoft-com:office:word" xmlns:m="http://schemas.microsoft.com/office/2004/12/omml"><cmn:script>context.docw.d.nhtml = node;</cmn:script>

<head><cmn:script>context.docw.d.nhead = node;</cmn:script>
